{
    "name":"Adding Numbers",
    "version":"1.0",
    "description":"Adds everytime you click",
    "manifest_version": 2,
    "permissions": [
          "storage"
        ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action":{
      "default_popup":"popup/popup.html"
    }
}
